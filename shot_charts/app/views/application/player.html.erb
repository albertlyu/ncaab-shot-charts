<div class="stripe">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h3><%= @player.player_first_name %> <%= @player.player_last_name %></h3>
        <ul>
        	<%= @plays.each do |play| %>
	        <li><%= play.details %></a></li>
	        <% end %>
        </ul>
      </div>
      <div class="col-md-6">
        <h3>Game Log</h3>
        <ul>
          <%= @playergames.each do |playergame| %>
          <li><a href="#"><%= playergame.game_id %></a></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>
<br>